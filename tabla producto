CREATE TABLE public.producto
(
    id_producto integer NOT NULL DEFAULT nextval('producto_id_producto_seq'::regclass),
    nombre_producto character varying(100) COLLATE pg_catalog."default" NOT NULL,
    "descripci√≥n" text COLLATE pg_catalog."default",
    precio numeric(10,2) NOT NULL,
    stock integer NOT NULL,
    id_locatario integer,
    fecha_agregado date NOT NULL,
    CONSTRAINT producto_pkey PRIMARY KEY (id_producto),
    CONSTRAINT producto_id_locatario_fkey FOREIGN KEY (id_locatario)
        REFERENCES public.locatario (id_locatario) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE CASCADE
)
WITH (
    OIDS = FALSE
)
TABLESPACE pg_default;

ALTER TABLE public.producto
    OWNER to postgres;
